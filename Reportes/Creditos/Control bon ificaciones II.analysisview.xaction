          <chart-conf/>  
          <spacer/>  
          <print-conf/>  
          <print-pdf/>  
          <spacer/>  
          <excel/> 
        </options>  
        <query><![CDATA[with member [Measures].[Diferencia Mes Anterior] as '((([DetalleMes].CurrentMember, [Measures].[Porcentaje de Bonificaciones]) / ([DetalleMes].CurrentMember, [Measures].[Porcentaje de Bonificaciones])) - (([DetalleMes].CurrentMember, [Measures].[Porcentaje de Bonificaciones]) / ([DetalleMes].CurrentMember.PrevMember, [Measures].[Porcentaje de Bonificaciones])))', format_string = IIf(((([DetalleMes].CurrentMember, [Measures].[Porcentaje de Bonificaciones]) / ([DetalleMes].CurrentMember.PrevMember, [Measures].[Porcentaje de Bonificaciones])) < 1.0), "|0.00 %|style='green'", "|0.00 %|style='yellow'")
select NON EMPTY {[Measures].[ImporteNeto], [Measures].[Cantidad], [Measures].[% Otros Descuentos], [Measures].[% Desc en Factura], [Measures].[Porcentaje de Bonificaciones], [Measures].[Diferencia Mes Anterior]} ON COLUMNS,
  NON EMPTY Hierarchize(Union({[DetalleMes].[Meses]}, [DetalleMes].[Meses].Children)) ON ROWS
from [DetalleVentas]
where Crossjoin({[Ano].[2014], [Ano].[2015]}, {[Cliente].[AUTOSERVICIO MAYORISTA DIARCO], [Cliente].[AUTOSERVICIO MAYORISTA DIARCO S.A.], [Cliente].[COTO CIC SA (PROV. 628)], [Cliente].[INC S.A.], [Cliente].[JUMBO RETAIL ARGENTINA S.A.], [Cliente].[MAXICONSUMO S.A.], [Cliente].[MAYCAR S.A.], [Cliente].[SA. IMP. Y EXP. LA PATAGONIA], [Cliente].[SUP. MAY YAGUAR], [Cliente].[SUPERMERCADO MAY MAKRO S.A], [Cliente].[WAL-MART ARGENTINA S.R.L.]})
]]></query>  
        <jndi><![CDATA[DosAnclas_DWH]]></jndi>  
        <cube>DetalleVentas</cube>
