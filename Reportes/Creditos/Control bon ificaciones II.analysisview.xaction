        <title><![CDATA[Control bon ificaciones II]]></title>  
        <viewer>Pivot</viewer>  
        <model><![CDATA[solution:/DosAnclas/modelos/dosanclas.mondrian.xml]]></model>  
        <options> 
          <personal/>  
          <cube-nav/>  
          <mdx-edit/>  
          <sort-conf/>  
          <spacer/>  
          <level-style/>  
          <hide-spans/>  
          <properties/>  
          <non-empty/>  
          <swap-axes/>  
          <spacer/>  
          <drill-member/>  
          <drill-position/>  
          <drill-replace/>  
          <drill-thru/>  
